        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.scy.mapper.ArticleMapper">

    <resultMap id="BaseResultMap" type="com.scy.pojo.Article">
            <id property="articleId" column="article_id" jdbcType="INTEGER"/>
